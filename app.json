{
  "expo": {
    "name": "Qourtify",
    "slug": "qourtify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.quiqueru.qourtify",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "google-oauth",
            "CFBundleURLSchemes": [
              "com.quiqueru.qourtify"
            ]
          },
          {
            "CFBundleURLName": "qourtify-auth",
            "CFBundleURLSchemes": [
              "qourtify"
            ]
          },
          {
            "CFBundleURLName": "google-oauth",
            "CFBundleURLSchemes": [
              "com.quiqueru.qourtify"
            ]
          },
          {
            "CFBundleURLName": "qourtify-auth",
            "CFBundleURLSchemes": [
              "qourtify"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.quiqueru.qourtify",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "qourtify.com"
            },
            {
              "scheme": "qourtify"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "qourtify",
    "extra": {
      "eas": {
        "projectId": "048a9614-92a6-4c9b-9da1-2b8d81ff1906"
      }
    },
    "notification": {
      "icon": "./assets/icon.png"
    }
  }
}
