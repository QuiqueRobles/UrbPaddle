{
  "expo": {
    "name": "Qourtify",
    "slug": "qourtify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1A3C34"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.quiqueru.qourtify",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "google-oauth",
            "CFBundleURLSchemes": [
              "com.quiqueru.qourtify"
            ]
          },
          {
            "CFBundleURLName": "qourtify-auth",
            "CFBundleURLSchemes": [
              "qourtify"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.quiqueru.qourtify",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "qourtify.com"
            },
            {
              "scheme": "qourtify"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "qourtify",
    "extra": {
      "eas": {
        "projectId": "048a9614-92a6-4c9b-9da1-2b8d81ff1906"
      }
    },
    "notification": {
      "icon": "./assets/icon.png",
      "iosDisplayInForeground": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#1A3C34",
          "sounds": []
        }
      ]
    ]
  }
}